<dashboard theme="dark">
  <label>AppScopeClassicDashboard</label>
  <row>
    <panel>
      <title>SSHD which process was started</title>
      <table>
        <title>Table contains the information about user and proc</title>
        <search>
          <query>metric = proc.start | rename proc as proc, username as user | table user proc _time</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
</dashboard>