<dashboard theme="dark">
  <label>AppScopeClassicDashboard</label>
  <row>
    <panel>
      <title>Proc memory in time</title>
      <chart>
        <search>
          <query>container_name="appscope02" source="proc.mem" "data._value"="*"</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
</dashboard>